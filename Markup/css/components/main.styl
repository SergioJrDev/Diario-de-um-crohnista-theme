* {
	margin 0;
	padding 0
	box-sizing: border-box;
	font-style: normal;
	text-decoration: none;
	outline: none;
	list-style-type: none;
	font-synthesis: none;
  	text-rendering: optimizeLegibility;
  	-webkit-font-smoothing: antialiased;
  	-moz-osx-font-smoothing: grayscale;
}

a {
	cursor: pointer;
}

html, body {
	height: 100%;
	font-family: 'Oxygen', sans-serif;
	font-size: 15px;
}

.hidden {
	display: none;
}

img {
	max-width: 100%;
	width: auto;
	height: auto;
}

.auto-h {
	height: 100vh
	.container {
		height: 100%;
		display: flex;
	    flex-direction: row;
	    justify-content: space-between;
	    align-items: flex-end;
	}
	.title_page {
		position: absolute;
	    top: 45%;
	    right: 0;
	    left: 0;
	}
}

.container	{
	max-width: 1170px
	padding 0 15px;
	margin 0 auto
	position relative
}

.title_page {
	font-family: 'Fjalla One', sans-serif;
	color: color_theme;
    text-transform: uppercase;
    text-align: center;
    margin-bottom: 20px;
}

.sub_title {
	max-width: 620px;
	margin: 0px auto 40px;
}
p {
	margin-bottom: 10px;
	color: color_p;
}


/* PROPRIEDADES */
.float-right {
	float: right;
}

.border-bottom {
	border-bottom: 1px solid #c5c5c5;
    padding-bottom: 10px;
    margin-bottom: 10px;
}

.top-left-radius {
	border-radius: 5px 0 0 0;
}

.top-right-radius {
	border-radius: 0 5px 0 0;
}

.bottom-left-radius {
	border-radius: 0 0 0 5px;
}

.bottom-right-radius {
	border-radius: 0 0 5px 0;
}

.space-default {
	padding: 50px 0;
}

.clearfix {
	clear: both;
}

.mg-bottom {
	margin-bottom: 40px;
}

.space-default-top {
	padding-top: 50px;
}

.flex {
	display: flex;
	flex-direction: column;
}


@media screen and (min-width: sm) {
	.space-default {
		/*padding: 100px 0;*/
	}
}
